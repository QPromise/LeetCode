#明天再来追写，方法实现参考链接https://mp.weixin.qq.com/s/3h9iqU4rdH3EIy5m6AzXsg
class Solution(object):
    def climbStairs(self, n,map={}):
        """
        :type n: int
        :rtype: int
        """
        if n==1:
            return 1
        if n==2:
            return 2
        if map.has_key(n):
            return map[n]
        else:
            value=self.climbStairs(n-1)+self.climbStairs(n-2)
            map[n]=value
            return value
